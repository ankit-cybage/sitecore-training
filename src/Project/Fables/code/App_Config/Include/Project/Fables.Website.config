<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sites>
    <site name="fables" patch:after="site[@name='modules_website']"
          targetHostName="fables.dev.local"
          database="web"
          virtualFolder="/"
          physicalFolder="/"
          rootPath="/sitecore/content/Fables"
          startItem="/Home"
          domain="extranet"
          allowDebug="true"
          cacheHtml="true"
          htmlCacheSize="50MB"
          registryCacheSize="0"
          viewStateCacheSize="0"
          xslCacheSize="25MB"
          filteredItemsCacheSize="10MB"
          enablePreview="true"
          enableWebEdit="true"
          enableDebugger="true"
          disableClientData="false"
          cacheRenderingParameters="true"
          renderingParametersCacheSize="10MB"
          enableItemLanguageFallback="true" />
  </sites>
  </sitecore>
</configuration>
